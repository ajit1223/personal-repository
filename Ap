package loop;
import java.util.*;
public class AP {
	public static void main(String[] args) {
		try (Scanner sc = new Scanner(System.in)) {
			 int x,n=1,sum,i=1;
			x=sc.nextInt();
			while(n<=x && n<=1000){
				// this is a formula we have given
				sum=(3*i)+2;
				if(sum%4!=0){
	               System.out.print(sum+" ");
				   n++;
				}
				i++;
			}
			
		
		}
	}
}


